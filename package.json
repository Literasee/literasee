{
  "name": "literasee",
  "version": "1.0.0",
  "description": "Literasee platform viewer and editor",
  "main": "index.js",
  "engines": {
    "node": "5.6.0",
    "npm": "3.6.0"
  },
  "scripts": {
    "dev": "nodemon -w src/routes/api -w src/routes/view src/index.js --exec babel-node src/server.js",
    "prebuild": "stylus -c -u yeticss src/live-preview.styl -o public/live-preview.css",
    "build": "webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "prestart": "rm -rf dist && babel src --out-dir dist && npm run copy",
    "copy": "cp src/routes/edit/index.html dist/routes/edit/index.html && cp -R src/views dist/views",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Literasee/literasee.git"
  },
  "author": "Ben Clinkinbeard <ben.clinkinbeard@gmail.com> (http://benclinkinbeard.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Literasee/literasee/issues"
  },
  "homepage": "https://github.com/Literasee/literasee#readme",
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "brace": "^0.7.0",
    "classnames": "^2.2.3",
    "clipboard-js": "^0.2.0",
    "cloudinary": "^1.3.0",
    "consolidate": "^0.13.1",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-subdomain": "^1.0.4",
    "formidable": "^1.0.17",
    "handlebars": "^4.0.5",
    "image-size-stream": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "mongoose": "^4.3.6",
    "parse-katex": "^0.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.6",
    "react-ace": "^3.1.0",
    "react-dom": "^0.14.6",
    "react-dropzone": "^3.3.2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.53.0",
    "stylus-loader": "^1.5.0",
    "superagent": "^1.7.1",
    "superagent-bluebird-promise": "^3.0.0",
    "superagent-proxy": "^1.0.0",
    "webpack": "^1.12.11",
    "yeticss": "^7.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "nodemon": "^1.9.1",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
